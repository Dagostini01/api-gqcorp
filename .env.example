# Configuração do Banco de Dados
# Copie este arquivo para .env e configure suas credenciais

# PostgreSQL Database URL
# Formato: postgresql://username:password@localhost:5432/database_name
DATABASE_URL="postgresql://username:password@localhost:5432/api_gqcorp"

# Configurações da API
PORT=3000
NODE_ENV=development
AUTH_SECRET=changeme-super-secret

# Twilio (SMS real)
# Preencha para enviar SMS real via Twilio
# Dica: o número deve estar em formato E.164 (ex.: +15551234567)
TWILIO_ACCOUNT_SID=ACXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX
TWILIO_AUTH_TOKEN=your_auth_token
TWILIO_FROM=+15551234567

# Configurações dos Robôs
PYTHON_PATH=python

# Configurações de Cache (opcional)
REDIS_URL=redis://localhost:6379

# Configurações de Log
LOG_LEVEL=info