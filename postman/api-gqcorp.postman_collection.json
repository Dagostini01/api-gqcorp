    {
      "name": "Auth",
      "item": [
        {
          "name": "POST /auth/login",
          "request": {
            "method": "POST",
            "header": [
              { "key": "Content-Type", "value": "application/json" }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"dev.user@example.com\",\n  \"password\": \"Str0ng!Pass\"\n}"
            },
            "url": {
              "raw": "http://localhost:3000/auth/login",
              "protocol": "http",
              "host": ["localhost"],
              "port": "3000",
              "path": ["auth", "login"]
            }
          },
          "event": [
            {
              "listen": "test",
              "script": {
                "type": "text/javascript",
                "exec": [
                  "pm.test('Status code is 200', function () {",
                  "  pm.response.to.have.status(200);",
                  "});",
                  "var json = pm.response.json();",
                  "pm.test('Response has token e user', function () {",
                  "  pm.expect(json).to.have.property('token');",
                  "  pm.expect(json).to.have.property('user');",
                  "});",
                  "if (json.token) { pm.environment.set('authToken', json.token); pm.environment.set('userId', json.user.id); }"
                ]
              }
            }
          ]
        }
      ]
    },
