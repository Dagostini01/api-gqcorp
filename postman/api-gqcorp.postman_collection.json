        {
          "name": "POST /brasil/importar",
          "request": {
            "method": "POST",
            "header": [
              { "key": "Content-Type", "value": "application/json" }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"ncm\": \"34021300\",\n  \"data_de\": \"2022-01\",\n  \"data_ate\": \"2022-03\",\n  \"limit\": 100\n}"
            },
            "url": {
              "raw": "http://localhost:3000/brasil/importar",
              "protocol": "http",
              "host": ["localhost"],
              "port": "3000",
              "path": ["brasil", "importar"]
            }
          },
          "event": [
            {
              "listen": "test",
              "script": {
                "type": "text/javascript",
                "exec": [
                  "pm.test('Status code is 200', function () {",
                  "  pm.response.to.have.status(200);",
                  "});",
                  "pm.test('Response has total and persisted only (no resultados)', function () {",
                  "  var json = pm.response.json();",
                  "  pm.expect(json).to.have.property('total');",
                  "  pm.expect(json).to.have.property('persisted');",
                  "  pm.expect(json.resultados).to.eql(undefined);",
                  "});"
                ]
              }
            }
          ]
        }
      ]
