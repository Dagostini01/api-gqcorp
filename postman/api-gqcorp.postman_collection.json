{
  "info": {
    "name": "API GQCorp",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Health",
      "item": [
        {
          "name": "GET /health",
          "request": {
            "method": "GET",
            "url": {
              "raw": "http://localhost:3000/health",
              "protocol": "http",
              "host": ["localhost"],
              "port": "3000",
              "path": ["health"]
            }
          },
          "event": [
            {
              "listen": "test",
              "script": {
                "type": "text/javascript",
                "exec": [
                  "pm.test('Status code is 200', function () {",
                  "  pm.response.to.have.status(200);",
                  "});",
                  "pm.test('Response has status ok', function () {",
                  "  var json = pm.response.json();",
                  "  pm.expect(json.status).to.eql('ok');",
                  "});"
                ]
              }
        }
      ]
        },
        {
          "name": "GET /brasil (paginação e filtro NCM)",
          "request": {
            "method": "GET",
            "url": {
              "raw": "http://localhost:3000/brasil?page=1&pageSize=2&ncm=34021300",
              "protocol": "http",
              "host": ["localhost"],
              "port": "3000",
              "path": ["brasil"],
              "query": [
                { "key": "page", "value": "1" },
                { "key": "pageSize", "value": "2" },
                { "key": "ncm", "value": "34021300" }
              ]
            }
          },
          "event": [
            {
              "listen": "test",
              "script": {
                "type": "text/javascript",
                "exec": [
                  "pm.test('Status code is 200', function () {",
                  "  pm.response.to.have.status(200);",
                  "});",
                  "pm.test('Response has pagination metadata', function () {",
                  "  var json = pm.response.json();",
                  "  pm.expect(json).to.have.property('total');",
                  "  pm.expect(json).to.have.property('page');",
                  "  pm.expect(json).to.have.property('pageSize');",
                  "  pm.expect(json).to.have.property('totalPages');",
                  "  pm.expect(json).to.have.property('resultados');",
                  "  pm.expect(json.page).to.be.a('number');",
                  "  pm.expect(json.pageSize).to.be.a('number');",
                  "  pm.expect(json.resultados.length).to.be.at.most(json.pageSize);",
                  "});"
                ]
              }
            }
          ]
        }
      ]
    },
